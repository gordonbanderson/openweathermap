{
  "name": "weboftalent/openweathermap",
  "description": "SilverStripe module for Open Weather Map",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "weather",
    "weather forecasts",
    "weather history",
    "weboftalent"
  ],
  "authors": [
    {
      "name": "Gordon Anderson",
      "email": "gordon.b.anderson@gmail.com",
      "role": "Developer"
    }
  ],
	"require": {
		"php" : "~5.6|~7.0",
		"silverstripe/framework": "^4.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"squizlabs/php_codesniffer": "^2.3"
	},
	"autoload": {
		"psr-4": {
			"WebOfTalent\\OpenWeatherMap\\": "src/",
			"WebOfTalent\\Staff\\OpenWeatherMap\\": "tests/"
		}
	},
	"extra": {
		"expose": [
			"css",
			"icons"
		]
	}
	,
	"license": "BSD-3-Clause",
	"minimum-stability": "dev",
	"prefer-stable" : true
}
